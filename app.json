{
  "expo": {
    "name": "منصة التعليم",
    "slug": "learning-platform",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.apoalm13.learningplatform"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c4cdf2d7-9896-44fa-80ae-04304f7f3bb8"
      }
    }
  }
}
